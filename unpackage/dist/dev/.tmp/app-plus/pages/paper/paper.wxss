
.body{
	padding:0 20rpx;
}
.papar-left-popup-mask{
	position:fixed;
	top:0;
	bottom:0;
	left:0;
	right:0;
	z-index:1999;
}
.papar-left-popup{
	position:fixed;
	right:0;
	top:10rpx;
	background:#FFFFFF;
	z-index:2000;
	width:55%;
	-webkit-box-shadow: 1rpx 1rpx 20rpx 2rpx #CCCCCC;
	        box-shadow: 1rpx 1rpx 20rpx 2rpx #CCCCCC;
}
.papar-left-popup>view{
	padding:20rpx;
	font-size:35rpx;
}
.papar-left-popup>view>view{
	margin-right:10rpx;
	font-weight: bold;
}
.papar-left-popup-h{
	background: #EEEEEE;
}


